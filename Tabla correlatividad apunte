CREATE TABLE IF NOT EXISTS `UASAL`.`CORRELATIVIDAD` (
  `car_id` INT NOT NULL,
  `mat_id` INT NOT NULL,
  `cor_idcorrelativacursar` INT NOT NULL,
  `cor_idcorrelativafinal` INT NULL,
  PRIMARY KEY (`car_id`, `mat_id`, `cor_idcorrelativacursar`),
  CONSTRAINT `fk_CORRELATIVIDAD_MATERIA_X_CARRERA1`
    FOREIGN KEY (`mat_id` , `car_id`)
    REFERENCES `UASAL`.`MATERIA_X_CARRERA` (`mat_id` , `car_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;

SHOW WARNINGS;
CREATE INDEX `fk_CORRELATIVIDAD_MATERIA_X_CARRERA1_idx` ON `UASAL`.`CORRELATIVIDAD` (`mat_id` ASC, `car_id` ASC);

SHOW WARNINGS;
